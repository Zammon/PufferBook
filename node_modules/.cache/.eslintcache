      \r\n        return(\r\n                <div className=\"slide-item\">\r\n                    <div className=\"slide-item-title\">\r\n                        {cover.title}\r\n                        <p className=\"slide-item-syn\">{cover.synopsis}</p>\r\n                    </div>\r\n                    <div className=\"slide-shadow\"></div>\r\n                    <img className=\"slide-item-img\" src={cover.imgs} />\r\n                </div>\r\n            );   \r\n    }\r\n\r\n    function resetTimeout() {\r\n        if(timeoutRef.current) {\r\n            clearTimeout(timeoutRef.current);\r\n        }\r\n    }\r\n\r\n    function Butslide(props) {\r\n        return(\r\n            <div className={`but-item${slidess===props.inx ? \"-active\" : \"\"}`} onClick={()=>{setSlidess(props.inx)}}></div>\r\n        );\r\n    }\r\n\r\n\r\n    return(\r\n        <div className=\"slide-container\">\r\n                <div className=\"slideshow-container\" style={{ transform: `translate3d(${-slidess * 100}%, 0, 0)`}}>\r\n                    {covermaps}\r\n                </div>\r\n            <div className=\"but-container\">\r\n                {pointmaps}\r\n            </div>\r\n        </div>\r\n    );  \r\n\r\n}\r\n\r\nexport default Slideshow","C:\\Users\\hongn\\Desktop\\Coding-Projects\\pufferbook\\src\\component\\Home\\Noveldaily.js",["135","136","137","138"],[],"C:\\Users\\hongn\\Desktop\\Coding-Projects\\pufferbook\\src\\component\\Tools\\Butslide.js",["139","140","141","142"],[],"import { Component, useState } from \"react\";\r\nimport React from \"react\";\r\nimport './../../css/Tools/Butslide.css';\r\nimport iconLeft from './../../imagins/tools/left_button.png';\r\nimport iconRight from './../../imagins/tools/right_button.png';\r\n\r\nconst ButslideLeft = (props) => { \r\n    return(\r\n        <div className=\"but-slides-click but-left\" onClick={props.onclickset}>\r\n            <img className=\"img-iconslide\" src={iconLeft}/>\r\n        </div>\r\n    );\r\